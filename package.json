{
  "private": true,
  "name": "purescript-emo8",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist output .spago .cache .psci_modules",
    "build": "spago build",
    "example:bundle:hello:js": "spago bundle-app --path example/hello/Main.purs --to dist/example/hello/index.js",
    "example:bundle:hello:css": "cpx asset/index.css dist/example/hello",
    "example:bundle:hello:html": "cpx asset/index.html dist/example/hello",
    "example:bundle:hello": "run-s example:bundle:hello:*",
    "example:build:hello": "spago build --path example/hello/Main.purs",
    "example:build:basic": "spago build --path example/basic/Main.purs",
    "example:build:emodius": "spago build --path \"example/emodius/**/*.purs\"",
    "dev": "parcel serve asset/index.html"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "purescript": "^0.13.6",
    "rimraf": "^3.0.2",
    "spago": "^0.15.2"
  }
}